<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BaseShapeAppearanceBottomSheetDialog">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">28dp</item>
        <item name="cornerSizeTopLeft">28dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="GreenMenuToolbarStyle" parent="ToolbarStyle">
        <item name="actionMenuTextColor">@color/teal_700</item>
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.DayNight" />
    <style name="ToolbarStyle" parent="AppTheme">
        <item name="android:textColorPrimary">@color/black</item>
        <item name="android:textColorSecondary">@color/black</item>
        <item name="actionMenuTextColor">@color/black</item>
    </style>
    <style name="AppTheme" parent="AppThemeMore" />

    <style name="AppThemeMore" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/teal_700</item>
        <item name="colorPrimaryDark">@color/teal_700</item>
        <item name="colorAccent">@color/teal_700</item>

        <item name="android:windowBackground">@color/teal_700</item>
    </style>

</resources>